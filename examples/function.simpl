"func add(int a, int b) {"
"    return a + b;"
"}"

"func mul(int a, int b) {"
"     return a * b;"
"}"

"func main() {"
"    int a;"
"    int b;"
"    a = add(10, 2);"
"    print(a);"
"    b = mul(a, a + b);"
"    print(b);"
"}"

fn ADD (#A _ #B){
    <- A p B
}

fn MULT (#A _ #B){
    <- A m B
}

START {
    #A e ADD(10 _ 2)
    < A
    #B e MULT(A _ A p B)
    < B
}